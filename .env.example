# Postgres Configuration
DATABASE_USER=app_user
DATABASE_PW=app_pw
DATABASE_NAME=app_db

# Qdrant Configuration
# Optional profile selector for connection-only overrides: dev or prod
QDRANT_ENV=dev

# Dev connection overrides (optional)
QDRANT_DEV_MODE=server
QDRANT_DEV_URL=http://localhost:6333
QDRANT_DEV_API_KEY=

# Prod connection overrides (optional)
QDRANT_PROD_MODE=server
QDRANT_PROD_URL=
QDRANT_PROD_API_KEY=

# Generic connection fallback (used when profile-specific value is missing)
QDRANT_MODE=server
QDRANT_URL=
QDRANT_HOST=localhost
QDRANT_PORT=6333
QDRANT_API_KEY=
QDRANT_HTTPS=false
QDRANT_PATH=./qdrant_data
QDRANT_TIMEOUT=10.0

# Shared Qdrant collection/index tuning 
QDRANT_VECTOR_SIZE=768
QDRANT_BOOKS_ON_DISK=true
QDRANT_BOOKS_INT8_QUANTIZATION=true
QDRANT_BOOKS_QUANTILE=0.99
QDRANT_BOOKS_QUANT_ALWAYS_RAM=true
QDRANT_BOOKS_HNSW_ON_DISK=true
QDRANT_BOOKS_HNSW_M=16
# Hugging Face token for accessing embedding models
HF_TOKEN=
